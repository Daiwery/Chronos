<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeTextView">
        <attr name="timeLocal" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="local" value="1"/>
        </attr>
        <attr name="timeStyle" format="enum">
            <enum name="fullFormat" value="0"/>
            <enum name="longFormat" value="1"/>
            <enum name="mediumFormat" value="2"/>
            <enum name="shortFormat" value="3"/>
        </attr>
        <attr name="timeType" format="enum">
            <enum name="time" value="0"/>
            <enum name="date" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SelectTypeShowing">
    </declare-styleable>

    <attr name="stripWidth" format="dimension" />
    <attr name="spaceWidth" format="dimension" />
    <attr name="corner" format="dimension" />
    <attr name="colorColumn" format="color" />

    <declare-styleable name="SelectActionTypeView">
    </declare-styleable>

    <declare-styleable name="ScheduleView">
        <attr name="stripWidth"/>
        <attr name="spaceWidth"/>
        <attr name="corner"/>
        <attr name="colorColumn"/>
    </declare-styleable>

    <declare-styleable name="MultiScheduleView">
        <attr name="stripWidth"/>
        <attr name="spaceWidth"/>
        <attr name="corner"/>
        <attr name="colorColumn"/>
        <attr name="colorError" format="color" />
    </declare-styleable>

    <declare-styleable name="ActionsView">
        <attr name="stripWidth"/>
        <attr name="spaceWidth"/>
        <attr name="corner"/>
        <attr name="colorColumn"/>
    </declare-styleable>

    <declare-styleable name="ClockFaceView">
        <attr name="textSize" format="dimension" />
        <attr name="spaceHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ScheduleClockView">
    </declare-styleable>

    <declare-styleable name="ActionsClockView">
    </declare-styleable>
</resources>